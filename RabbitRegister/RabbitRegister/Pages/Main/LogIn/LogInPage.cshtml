@page
@model RabbitRegister.Pages.Main.LogIn.LogInPageModel
@{
}

<head>
    <!-- Styling reference -->
    <link rel="stylesheet" href="/css/site.css" />
</head>

<body class="w3-auto">
    <div class="min-vh-100" style="margin-left: 45%; margin-right: 45%;"
        <br><br><br><br><br>
        

        <div class="w3-main">

            <div>
                @Model.Message
            </div>
            <form method="post">
                <div class="form-group">
                    <label asp-for="BreederRegNo" class="col-form-label "></label>
                    <div>
                        <input asp-for="BreederRegNo" value="" />
                    </div>
                </div>
                <div class="form-group">
                    <label asp-for="Password" class="col-form-label"></label>
                    <div>
                        <input asp-for="Password" />
                    </div>
                </div>
                <br>
                <div class="form-group">
                    <button class="btn btn-outline-light">Log in</button>
                </div>
            </form>

        </div>
    </div>
</body>