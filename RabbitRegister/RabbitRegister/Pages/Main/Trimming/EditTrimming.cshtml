@page
@model RabbitRegister.Pages.Main.Trimming.EditTrimmingModel
@{
}

<form method="post">
	<div class="form-group">
		<label asp-for="Trimming.TrimmingId" class="control-lable"></label>
		<input asp-for="Trimming.TrimmingId" class="form-control" readonly="@(true)"/>
	</div>
	<div class="form-group">
		<label asp-for="Trimming.RabbitRegNo" class="control-lable"></label>
		<input asp-for="Trimming.RabbitRegNo" class="form-control" />
		<span asp-validation-for="Trimming.RabbitRegNo" class="text-danger"></span>
	</div>
		<div class="form-group">
		<label asp-for="Trimming.BreederRegNo" class="control-lable"></label>
		<input asp-for="Trimming.BreederRegNo" class="form-control" />
		<span asp-validation-for="Trimming.BreederRegNo" class="text-danger"></span>
	</div>
	<div class="form-group">
		<label asp-for="Trimming.Name" class="control-lable"></label>
		<input asp-for="Trimming.Name" class="form-control" />
		<span asp-validation-for="Trimming.Name" class="text-danger"></span>
	</div>
		<div class="form-group">
		<label asp-for="Trimming.Date" class="control-lable"></label>
		<input asp-for="Trimming.Date" class="form-control" />
		<span asp-validation-for="Trimming.Date" class="text-danger"></span>
	</div>
		<div class="form-group">
		<label asp-for="Trimming.TimeUsed" class="control-lable"></label>
		<input asp-for="Trimming.TimeUsed" class="form-control" />
		<span asp-validation-for="Trimming.TimeUsed" class="text-danger"></span>
	</div>
		<div class="form-group">
		<label asp-for="Trimming.HairLengthByDayNinety" class="control-lable"></label>
		<input asp-for="Trimming.HairLengthByDayNinety" class="form-control" />
		<span asp-validation-for="Trimming.HairLengthByDayNinety" class="text-danger"></span>
	</div>
		<div class="form-group">
		<label asp-for="Trimming.WoolDensity" class="control-lable"></label>
		<input asp-for="Trimming.WoolDensity" class="form-control" />
		<span asp-validation-for="Trimming.WoolDensity" class="text-danger"></span>
	</div>
		<div class="form-group">
		<label asp-for="Trimming.FirstSortmentWeight" class="control-lable"></label>
		<input asp-for="Trimming.FirstSortmentWeight" class="form-control" />
		<span asp-validation-for="Trimming.FirstSortmentWeight" class="text-danger"></span>
	</div>
		<div class="form-group">
		<label asp-for="Trimming.SecondSortmentWeight" class="control-lable"></label>
		<input asp-for="Trimming.SecondSortmentWeight" class="form-control" />
		<span asp-validation-for="Trimming.SecondSortmentWeight" class="text-danger"></span>
	</div>
	<div class="form-group">
		<label asp-for="Trimming.DisposableWoolWeight" class="control-lable"></label>
		<input asp-for="Trimming.DisposableWoolWeight" class="form-control" />
		<span asp-validation-for="Trimming.DisposableWoolWeight" class="text-danger"></span>
	</div>

	<p>
		<div class="form-group">
			<input type="submit" value="Update" class="btn btn-primary" />
		</div>
	</p>
</form>

<p>
	<a asp-route-Owner="@User.Identity.Name" asp-page="/Main/Trimming/GetAllTrimming">Tilbage til klipninger</a>
</p>